import os
import subprocess
from eyed3 import *
import inspect
class Steve(object):
        def get_mp3(self):
                newpath = os.path.expanduser('~/Music')
                for root, dirs, files in os.walk(newpath):
                        return files
        def get_mp3_tags(self,path):
                tag = eyed3.Tag()
                tag.link(path)
